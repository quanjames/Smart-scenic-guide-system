{"ast":null,"code":"import request from \"../../utils/request\";\nexport default {\n  name: \"Spot\",\n  data() {\n    return {\n      tableData: [],\n      touristname: \"\",\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      form: {},\n      multipleSelection: [],\n      // 多选框，选定时的内容\n      dialogFormVisible: false,\n      // 增加功能对话框默认不展示\n      headerBg: 'headerBg'\n    };\n  },\n  methods: {\n    load() {\n      this.request.get(\"http://localhost:9090/spot/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          touristname: this.touristname\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records; // records 请看控制台的输出\n        this.total = res.total;\n        console.log(res);\n      });\n    },\n    save() {\n      this.request.post(\"http://localhost:9090/spot\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handelEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    delBatch() {\n      // let ids = this.multipleSelection.map(v => v.id)\n      let ids = this.multipleSelection.map(v => v.id);\n      console.log(ids);\n      this.request.post(\"http://localhost:9090/spot/del/batch\").then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleDel(id) {\n      this.request.delete(\"http://localhost:9090/spot/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    reset() {\n      this.touristname = \"\";\n      this.load();\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n  },\n  created() {\n    this.load();\n  }\n};","map":{"version":3,"names":["request","name","data","tableData","touristname","total","pageNum","pageSize","form","multipleSelection","dialogFormVisible","headerBg","methods","load","get","params","then","res","console","log","records","save","post","$message","success","error","handleAdd","handelEdit","row","delBatch","ids","map","v","id","handleDel","delete","reset","handleSelectionChange","val","handleSizeChange","handleCurrentChange","created"],"sources":["src/views/MangeView/Spot.vue"],"sourcesContent":["<template>\r\n<div>\r\n  <div style=\"margin: 10px 0\">\r\n    <el-input style=\"width: 200px\" placeholder=\"请输入景点\" suffix-icon=\"el-icon-search\" v-model=\"touristname\"></el-input>\r\n\r\n    <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n    <el-button class=\"ml-5\" type=\"danger\" @click=\"reset\">重置</el-button>\r\n\r\n  </div>\r\n  <div style=\"margin: 10px 0\">\r\n    <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n    <el-button type=\"danger\" @click=\"delBatch\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n\r\n\r\n    <el-upload action=\"http://localhost:9090/spot/import\" :show-file-list=false accept=\"xlsx\":on-success=\"handleImportSuccess\" style='display: inline-block'>\r\n      <el-button type=\"primary\" @click=\"\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n    </el-upload>\r\n\r\n\r\n    <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button>\r\n  </div>\r\n  <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n    <el-table-column\r\n        type=\"selection\"\r\n        width=\"55\"></el-table-column>\r\n    <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n    <el-table-column prop=\"touristname\" label=\"景点名\" width=\"140\"></el-table-column>\r\n    <el-table-column prop=\"lng\" label=\"经度\" width=\"120\"></el-table-column>\r\n    <el-table-column prop=\"lat\" label=\"维度\"></el-table-column>\r\n    <el-table-column prop=\"inconUrl\" label=\"图标url\"></el-table-column>\r\n    <el-table-column prop=\"imageUrl\" label=\"图片url\"></el-table-column>\r\n    <el-table-column prop=\"description\" label=\"景点描述\"></el-table-column>\r\n    <el-table-column label=\"操作\"  width=\"200\" align=\"center\">\r\n      <template slot-scope=\"scope\">\r\n        <el-button type=\"success\" @click=\"handelEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n        <el-popconfirm\r\n            class=\"ml-5\"\r\n            confirm-button-text='确定'\r\n            cancel-button-text='我再想想'\r\n            icon=\"el-icon-info\"\r\n            icon-color=\"red\"\r\n            title=\"您确定删除吗？\"\r\n            @confirm=\"handleDel(scope.row.id)\"\r\n        >\r\n          <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n        </el-popconfirm>\r\n      </template>\r\n    </el-table-column>\r\n  </el-table>\r\n  <div style=\"padding: 10px 0\">\r\n    <el-pagination\r\n      @size-change=\"handleSizeChange\"\r\n      @current-change=\"handleCurrentChange\"\r\n      :current-page=\"pageNum\"\r\n      :page-sizes=\"[2, 5, 10, 20]\"\r\n      :page-size=\"2\"\r\n      layout=\"total, sizes, prev, pager, next, jumper\"\r\n      :total=\"total\">\r\n    </el-pagination>\r\n\r\n  </div>\r\n  <!--          添加功能 对话框-->\r\n  <el-dialog title=\"景点信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\r\n    <el-form label-width=\"80px\" size=\"small\">\r\n      <el-form-item label=\"景点名\">\r\n        <el-input v-model=\"form.touristname\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"经度\">\r\n        <el-input v-model=\"form.lng\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"维度\">\r\n        <el-input v-model=\"form.lat\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"图标url\">\r\n        <el-input v-model=\"form.iconUrl\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"景点url\">\r\n        <el-input v-model=\"form.imageUrl\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"景点描述\">\r\n        <el-input v-model=\"form.description\" autocomplete=\"off\"></el-input>\r\n      </el-form-item>\r\n    </el-form>\r\n    <div slot=\"footer\" class=\"dialog-footer\">\r\n      <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n      <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n    </div>\r\n  </el-dialog>\r\n  \r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"../../utils/request\";\r\n\r\nexport default {\r\n  name: \"Spot\",\r\n  data(){\r\n    return{\r\n      tableData:[],\r\n      touristname:\"\",\r\n      total:0,\r\n      pageNum:1,\r\n      pageSize:2,\r\n      form:{},\r\n      multipleSelection: [], // 多选框，选定时的内容\r\n      dialogFormVisible:false, // 增加功能对话框默认不展示\r\n      headerBg: 'headerBg',\r\n\r\n    }\r\n  },\r\n  methods:{\r\n    load(){\r\n\r\n      this.request.get(\"http://localhost:9090/spot/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          touristname:this.touristname\r\n\r\n\r\n        }\r\n      }).then(\r\n          res=>{\r\n            console.log(res)\r\n            this.tableData = res.records // records 请看控制台的输出\r\n            this.total = res.total\r\n            console.log(res)\r\n          }\r\n      )\r\n    },\r\n    save(){\r\n      this.request.post(\"http://localhost:9090/spot\",this.form).then(res=>{\r\n      if(res){\r\n        this.$message.success(\"保存成功\")\r\n        this.dialogFormVisible=false\r\n        this.load()\r\n      }else{\r\n        this.$message.error(\"保存失败\")\r\n      }\r\n\r\n      })\r\n    },\r\n    handleAdd(){\r\n      this.dialogFormVisible=true;\r\n      this.form={}\r\n    },\r\n    handelEdit(row){\r\n      this.form=row\r\n      this.dialogFormVisible=true\r\n    },\r\n    delBatch(){\r\n     // let ids = this.multipleSelection.map(v => v.id)\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      console.log(ids)\r\n      this.request.post(\"http://localhost:9090/spot/del/batch\",).then(res=>{\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleDel(id){\r\n     this.request.delete(\"http://localhost:9090/spot/\"+id).then(res=>{\r\n       if (res) {\r\n         this.$message.success(\"删除成功\")\r\n\r\n         this.load()\r\n       } else {\r\n         this.$message.error(\"删除失败\")\r\n       }\r\n     })\r\n    },\r\n    reset(){\r\n      this.touristname=\"\"\r\n      this.load()\r\n    },\r\n    handleSelectionChange(val){\r\n      this.multipleSelection=val\r\n\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize;\r\n      this.load();\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum;\r\n      this.load();\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA6FA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,WAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,IAAA;MACAC,iBAAA;MAAA;MACAC,iBAAA;MAAA;MACAC,QAAA;IAEA;EACA;EACAC,OAAA;IACAC,KAAA;MAE