{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: \"Article\",\n  data() {\n    return {\n      form: {},\n      tableData: [],\n      name: '',\n      multipleSelection: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0,\n      dialogFormVisible: false,\n      teachers: [],\n      admin: localStorage.getItem(\"admin\") ? JSON.parse(localStorage.getItem(\"admin\")) : {},\n      content: '',\n      viewDialogVis: false\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    view(content) {\n      this.content = content;\n      this.viewDialogVis = true;\n    },\n    // 绑定@imgAdd event\n    imgAdd(pos, $file) {\n      let $vm = this.$refs.md;\n      // 第一步.将图片上传到服务器.\n      const formData = new FormData();\n      formData.append('file', $file);\n      axios({\n        url: 'http://localhost:9090/file/upload',\n        method: 'post',\n        data: formData,\n        headers: {\n          'Content-Type': 'multipart/form-data'\n        }\n      }).then(res => {\n        // 第二步.将返回的url替换到文本原位置![...](./0) -> ![...](url)\n        $vm.$img2Url(pos, res.data);\n      });\n    },\n    load() {\n      this.request.get(\"http://localhost:9090/article/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        // 为每篇文章设置发布人昵称\n        this.tableData.forEach(article => {\n          article.user = this.admin.adminNickname;\n        });\n        this.total = res.data.total;\n      });\n    },\n    changeEnable(row) {\n      this.request.post(\"http://localhost:9090/article/update\", row).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n        }\n      });\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogFormVisible = true;\n    },\n    del(id) {\n      this.request.delete(\"http://localhost:9090/article/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"http://localhost:9090/article/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    save() {\n      this.request.post(\"http://localhost:9090/article\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    },\n    download(url) {\n      window.open(url);\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","form","tableData","multipleSelection","pageNum","pageSize","total","dialogFormVisible","teachers","admin","localStorage","getItem","JSON","parse","content","viewDialogVis","created","load","methods","view","imgAdd","pos","$file","$vm","$refs","md","formData","FormData","append","url","method","headers","then","res","$img2Url","request","get","params","records","forEach","article","user","adminNickname","changeEnable","row","post","code","$message","success","handleAdd","handleEdit","stringify","del","id","delete","error","handleSelectionChange","val","console","log","delBatch","ids","map","v","save","reset","handleSizeChange","handleCurrentChange","download","window","open"],"sources":["src/views/MangeView/ArticleManage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\" >新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"\r\n              @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"文章标题\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"文章内容\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button @click=\"view(scope.row.content)\" type=\"primary\">查看内容</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"user\" label=\"发布人\"></el-table-column>\r\n      <el-table-column prop=\"time\" label=\"发布时间\"></el-table-column>\r\n      <el-table-column label=\"操作\" width=\"280\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\" >编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\" >删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"文章信息\" :visible.sync=\"dialogFormVisible\" width=\"60%\" >\r\n      <el-form label-width=\"80px\" size=\"small\">\r\n        <el-form-item label=\"文章标题\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"文章内容\">\r\n          <mavon-editor ref=\"md\" v-model=\"form.content\" :ishljs=\"true\" @imgAdd=\"imgAdd\"/>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"文章信息\" :visible.sync=\"viewDialogVis\" width=\"60%\" >\r\n      <el-card>\r\n        <div>\r\n          <mavon-editor\r\n              class=\"md\"\r\n              :value=\"content\"\r\n              :subfield=\"false\"\r\n              :defaultOpen=\"'preview'\"\r\n              :toolbarsFlag=\"false\"\r\n              :editable=\"false\"\r\n              :scrollStyle=\"true\"\r\n              :ishljs=\"true\"\r\n          />\r\n        </div>\r\n      </el-card>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Article\",\r\n  data() {\r\n    return {\r\n      form: {},\r\n      tableData: [],\r\n      name: '',\r\n      multipleSelection: [],\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      total: 0,\r\n      dialogFormVisible: false,\r\n      teachers: [],\r\n      admin: localStorage.getItem(\"admin\") ? JSON.parse(localStorage.getItem(\"admin\")) : {},\r\n      content: '',\r\n      viewDialogVis: false,\r\n\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    view(content) {\r\n      this.content = content\r\n      this.viewDialogVis = true\r\n    },\r\n    // 绑定@imgAdd event\r\n    imgAdd(pos, $file) {\r\n      let $vm = this.$refs.md\r\n      // 第一步.将图片上传到服务器.\r\n      const formData = new FormData();\r\n      formData.append('file', $file);\r\n      axios({\r\n        url: 'http://localhost:9090/file/upload',\r\n        method: 'post',\r\n        data: formData,\r\n        headers: {'Content-Type': 'multipart/form-data'},\r\n      }).then((res) => {\r\n        // 第二步.将返回的url替换到文本原位置![...](./0) -> ![...](url)\r\n        $vm.$img2Url(pos, res.data);\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"http://localhost:9090/article/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total;\r\n        // 为每篇文章设置发布人昵称\r\n        this.tableData.forEach(article => {\r\n          article.user = this.admin.adminNickname;\r\n        });\r\n\r\n        this.total = res.data.total;\r\n\r\n\r\n      })\r\n\r\n    },\r\n    changeEnable(row) {\r\n      this.request.post(\"http://localhost:9090/article/update\", row).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"操作成功\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form={}\r\n\r\n\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"http://localhost:9090/article/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"http://localhost:9090/article/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"http://localhost:9090/article\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":"AAqGA,OAAAA,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA;MACAC,SAAA;MACAH,IAAA;MACAI,iBAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,iBAAA;MACAC,QAAA;MACAC,KAAA,EAAAC,YAAA,CAAAC,OAAA,YAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,OAAA;MACAC,aAAA;IAEA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,KAAAL,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAC,aAAA;IACA;IACA;IACAK,OAAAC,GAAA,EAAAC,KAAA;MACA,IAAAC,GAAA,QAAAC,KAAA,CAAAC,EAAA;MACA;MACA,MAAAC,QAAA,OAAAC,QAAA;MACAD,QAAA,CAAAE,MAAA,SAAAN,KAAA;MACAxB,KAAA;QACA+B,GAAA;QACAC,MAAA;QACA9B,IAAA,EAAA0B,QAAA;QACAK,OAAA;UAAA;QAAA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA;QACAV,GAAA,CAAAW,QAAA,CAAAb,GAAA,EAAAY,GAAA,CAAAjC,IAAA;MACA;IACA;IACAiB,KAAA;MACA,KAAAkB,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAjC,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAN,IAAA,OAAAA;QACA;MACA,GAAAiC,IAAA,CAAAC,GAAA;QAEA,KAAA/B,SAAA,GAAA+B,GAAA,CAAAjC,IAAA,CAAAsC,OAAA;QACA,KAAAhC,KAAA,GAAA2B,GAAA,CAAAjC,IAAA,CAAAM,KAAA;QACA;QACA,KAAAJ,SAAA,CAAAqC,OAAA,CAAAC,OAAA;UACAA,OAAA,CAAAC,IAAA,QAAAhC,KAAA,CAAAiC,aAAA;QACA;QAEA,KAAApC,KAAA,GAAA2B,GAAA,CAAAjC,IAAA,CAAAM,KAAA;MAGA;IAEA;IACAqC,aAAAC,GAAA;MACA,KAAAT,OAAA,CAAAU,IAAA,yCAAAD,GAAA,EAAAZ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAa,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;QACA;MACA;IACA;IACAC,UAAA;MACA,KAAA1C,iBAAA;MACA,KAAAN,IAAA;IAGA;IACAiD,WAAAN,GAAA;MACA,KAAA3C,IAAA,GAAAW,IAAA,CAAAC,KAAA,CAAAD,IAAA,CAAAuC,SAAA,CAAAP,GAAA;MACA,KAAArC,iBAAA;IACA;IACA6C,IAAAC,EAAA;MACA,KAAAlB,OAAA,CAAAmB,MAAA,oCAAAD,EAAA,EAAArB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAa,IAAA;UACA,KAAAC,QAAA,CAAAC,OAAA;UACA,KAAA/B,IAAA;QACA;UA