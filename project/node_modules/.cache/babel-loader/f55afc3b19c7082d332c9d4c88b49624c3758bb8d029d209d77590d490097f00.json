{"ast":null,"code":"import request from \"../../utils/request\";\nexport default {\n  name: \"ShowManage\",\n  data() {\n    return {\n      tableData: [],\n      time: '',\n      name: \"\",\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      form: {\n        begainTime: new Date() // 设置初始值为当前时间\n      },\n\n      multipleSelection: [],\n      // 多选框，选定时的内容\n      dialogFormVisible: false,\n      // 增加功能对话框默认不展示\n      headerBg: 'headerBg',\n      selectedDate: null\n    };\n  },\n  methods: {\n    openDialog() {\n      this.dialogFormVisible = true;\n      // 设置时间回填的值\n      this.form.begainTime = new Date(\"2022-01-01 00:00:00\"); // 将日期时间值设置为需要回填的值\n    },\n\n    savea() {\n      // 在保存数据时，将form数据提交到后端\n      console.log(this.form);\n    },\n    load() {\n      this.request.get(\"http://localhost:9090/show/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records; // records 请看控制台的输出\n        this.total = res.total;\n        console.log(res);\n      });\n    },\n    save() {\n      this.request.post(\"http://localhost:9090/show\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    handleAvatarSuccess(res) {\n      this.form.imageUrl = res;\n      console.log(this.form.imageUrl);\n    },\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n    handelEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id);\n      console.log(ids);\n      this.request.post(\"http://localhost:9090/show/del/batch\", ids).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    handleDel(id) {\n      this.request.delete(\"http://localhost:9090/show/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n  },\n  created() {\n    this.load();\n    this.form.begainTime = new Date(); // 设置默认时间为当前时间\n  },\n\n  mounted() {}\n};","map":{"version":3,"names":["request","name","data","tableData","time","total","pageNum","pageSize","form","begainTime","Date","multipleSelection","dialogFormVisible","headerBg","selectedDate","methods","openDialog","savea","console","log","load","get","params","then","res","records","save","post","$message","success","error","handleAvatarSuccess","imageUrl","handleAdd","handelEdit","row","delBatch","ids","map","v","id","handleDel","delete","reset","handleSelectionChange","val","handleSizeChange","handleCurrentChange","created","mounted"],"sources":["src/views/MangeView/ShowManage.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入节目信息\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\r\n\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button class=\"ml-5\" type=\"danger\" @click=\"reset\">重置</el-button>\r\n\r\n    </div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\">新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      <el-button type=\"danger\" @click=\"delBatch\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n\r\n\r\n      <el-upload action=\"http://localhost:9090/spot/import\" :show-file-list=false accept=\"xlsx\":on-success=\"handleImportSuccess\" style='display: inline-block'>\r\n        <el-button type=\"primary\" @click=\"\" class=\"ml-5\">导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n\r\n\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\">导出 <i class=\"el-icon-top\"></i></el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column\r\n          type=\"selection\"\r\n          width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"节目名\" width=\"100\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"节目内容\" width=\"250\"></el-table-column>\r\n      <el-table-column prop=\"position\" label=\"举办位置\"></el-table-column>\r\n<!--      <el-table-column prop=\"imageUrl\" label=\"图片url\"></el-table-column>-->\r\n\r\n      <el-table-column prop=\"imageUrl\" label=\"图片url\">\r\n        <template slot-scope=\"scope\">.\r\n          <img :src=\"scope.row.imageUrl\" alt=\"\" style=\"width: 100px; border-radius: 0%; position: relative; top: 5px; right: 15px\">\r\n        </template>\r\n\r\n      </el-table-column>\r\n      <el-table-column prop=\"begainTime\" label=\"举行时间\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\"  width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handelEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"handleDel(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"2\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n    <!--          添加功能 对话框-->\r\n    <el-dialog title=\"节目信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\r\n      <el-form label-width=\"80px\" size=\"small\">\r\n        <el-form-item label=\"节目名称\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"节目内容\">\r\n          <el-input v-model=\"form.content\" type=\"textarea\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"节目地点\">\r\n          <el-input v-model=\"form.position\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"节目图片\">\r\n\r\n          <el-upload\r\n              class=\"avatar-uploader\"\r\n              action=\"http://localhost:9090/file/upload\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleAvatarSuccess\"\r\n          >\r\n            <img v-if=\" form.imageUrl\" :src=\"form.imageUrl\"  class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item label=\"节目时间\" >\r\n\r\n          <el-input v-model=\"form.begainTime\" autocomplete=\"off\"></el-input>\r\n\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"../../utils/request\";\r\n\r\nexport default {\r\n  name: \"ShowManage\",\r\n  data(){\r\n    return{\r\n      tableData:[],\r\n      time:'',\r\n      name:\"\",\r\n      total:0,\r\n      pageNum:1,\r\n      pageSize:2,\r\n      form: {\r\n        begainTime: new Date() // 设置初始值为当前时间\r\n      },\r\n      multipleSelection: [], // 多选框，选定时的内容\r\n      dialogFormVisible:false, // 增加功能对话框默认不展示\r\n      headerBg: 'headerBg',\r\n      selectedDate: null,\r\n\r\n\r\n    }\r\n  },\r\n  methods:{\r\n    openDialog() {\r\n      this.dialogFormVisible = true;\r\n      // 设置时间回填的值\r\n      this.form.begainTime = new Date(\"2022-01-01 00:00:00\"); // 将日期时间值设置为需要回填的值\r\n    },\r\n    savea() {\r\n      // 在保存数据时，将form数据提交到后端\r\n      console.log(this.form);\r\n    },\r\n    load(){\r\n\r\n      this.request.get(\"http://localhost:9090/show/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n\r\n          name:this.name,\r\n\r\n\r\n        }\r\n      }).then(\r\n          res=>{\r\n            console.log(res)\r\n            this.tableData = res.records // records 请看控制台的输出\r\n            this.total = res.total\r\n            console.log(res)\r\n          }\r\n      )\r\n    },\r\n    save(){\r\n      this.request.post(\"http://localhost:9090/show\",this.form).then(res=>{\r\n        if(res){\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible=false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n\r\n      })\r\n    },\r\n    handleAvatarSuccess(res) {\r\n      this.form.imageUrl = res;\r\n\r\n      console.log(this.form.imageUrl);\r\n    },\r\n    handleAdd(){\r\n      this.dialogFormVisible=true;\r\n      this.form={}\r\n    },\r\n    handelEdit(row){\r\n      this.form=row\r\n      this.dialogFormVisible=true\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      console.log(ids)\r\n      this.request.post(\"http://localhost:9090/show/del/batch\", ids).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    handleDel(id){\r\n      this.request.delete(\"http://localhost:9090/show/\"+id).then(res=>{\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.name=\"\"\r\n      this.load()\r\n    },\r\n    handleSelectionChange(val){\r\n      this.multipleSelection=val\r\n\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize;\r\n      this.load();\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum;\r\n      this.load();\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n    this.form.begainTime = new Date(); // 设置默认时间为当前时间\r\n  },\r\n  mounted(){\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.avatar-uploader {\r\n  text-align: center;\r\n  padding-bottom: 10px;\r\n}\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 138px;\r\n  height: 138px;\r\n  line-height: 138px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 138px;\r\n  height: 138px;\r\n  display: block;\r\n}\r\n</style>"],"mappings":"AA6GA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,IAAA;MACAH,IAAA;MACAI,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,IAAA;QACAC,UAAA,MAAAC,IAAA;MACA;;MACAC,iBAAA;MAAA;MACAC,iBAAA;MAAA;MACAC,QAAA;MACAC,YAAA;IAGA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAJ,iBAAA;MACA;MACA,KAAAJ,IAAA,CAAAC,UAAA,OAAAC,IAAA;IACA;;IACAO,MAAA;MACA;MACAC,OAAA,CAAAC,GAAA,MAAAX,IAAA;IACA;IACAY,KAAA;MAEA,KAAApB,OAAA,CAAAqB,GAAA;QACAC,MAAA;UACAhB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UAEAN,IAAA,OAAAA;QAGA;MACA,GAAAsB,IAAA,CACAC,GAAA;QACAN,OAAA,CAAAC,GAAA,CAAAK,GAAA;QACA,KAAArB,SAAA,GAAAqB,GAAA,CAAAC,OAAA;QACA,KAAApB,KAAA,GAAAmB,GAAA,CAAAnB,KAAA;QACAa,OAAA,CAAAC,GAAA,CAAAK,GAAA;MACA,EACA;IACA;IACAE,KAAA;MACA,KAAA1B,OAAA,CAAA2B,IAAA,oCAAAnB,IAAA,EAAAe,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAI,QAAA,CAAAC,OAAA;UACA,KAAAjB,iBAAA;UACA,KAAAQ,IAAA;QACA;UACA,KAAAQ,QAAA,CAAAE,KAAA;QACA;MAEA;IACA;IACAC,oBAAAP,GAAA;MACA,KAAAhB,IAAA,CAAAwB,QAAA,GAAAR,GAAA;MAEAN,OAAA,CAAAC,GAAA,MAAAX,IAAA,CAAAwB,QAAA;IACA;IACAC,UAAA;MACA,KAAArB,iBAAA;MACA,KAAAJ,IAAA;IACA;IACA0B,WAAAC,GAAA;MACA,KAAA3B,IAAA,GAAA2B,GAAA;MACA,KAAAvB,iBAAA;IACA;IACAwB,SAAA;MACA,IAAAC,GAAA,QAAA1B,iBAAA,CAAA2B,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA;MACAtB,OAAA,CAAAC,GAAA,CAAAkB,GAAA;MACA,KAAArC,OAAA,CAAA2B,IAAA,yCAAAU,GAAA,EAAAd,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAI,QAAA,CAAAC,OAAA;UACA,KAAAT,IAAA;QACA;UACA,KAAAQ,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IAEAW,UAAAD,EAAA;MACA,KAAAxC,OAAA,CAAA0C,MAAA,iCAAAF,EAAA,EAAAjB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAI,QAAA,CAAAC,OAAA;UAEA,KAAAT,IAAA;QACA;UACA,KAAAQ,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAa,MAAA;MACA,KAAA1C,IAAA;MACA,KAAAmB,IAAA;IACA;IACAwB,sBAAAC,GAAA;MACA,KAAAlC,iBAAA,GAAAkC,GAAA;IAEA;IACAC,iBAAAvC,QAAA;MACA,KAAAA,QAAA,GAAAA,QAAA;MACA,KAAAa,IAAA;IAEA;IACA2B,oBAAAzC,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAc,IAAA;IACA;EACA;EACA4B,QAAA;IACA,KAAA5B,IAAA;IACA,KAAAZ,IAAA,CAAAC,UAAA,OAAAC,IAAA;EA