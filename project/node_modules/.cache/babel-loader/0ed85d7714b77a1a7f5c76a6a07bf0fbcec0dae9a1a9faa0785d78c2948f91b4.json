el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"handleDel(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"2\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"../../utils/request\";\r\n\r\nexport default {\r\n  name: \"CommentManage\",\r\n  data(){\r\n    return{\r\n      tableData:[],\r\n      time:'',\r\n      name:\"\",\r\n      total:0,\r\n      pageNum:1,\r\n      pageSize:2,\r\n\r\n      multipleSelection: [], // 多选框，选定时的内容\r\n      dialogFormVisible:false, // 增加功能对话框默认不展示\r\n      headerBg: 'headerBg',\r\n      selectedDate: null,\r\n\r\n    }\r\n  },\r\n  methods:{\r\n\r\n\r\n    load(){\r\n\r\n      this.request.get(\"http://localhost:9090/comment/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n\r\n\r\n\r\n\r\n        }\r\n      }).then(\r\n          res=>{\r\n            console.log(res)\r\n            this.tableData = res.records // records 请看控制台的输出\r\n            this.total = res.total\r\n            console.log(res)\r\n          }\r\n      )\r\n    },\r\n\r\n\r\n\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)\r\n      console.log(ids)\r\n      this.request.post(\"http://localhost:9090/show/del/batch\", ids).then(res => {\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    handleDel(id){\r\n      this.request.delete(\"http://localhost:9090/comment/\"+id).then(res=>{\r\n        if (res) {\r\n          this.$message.success(\"删除成功\")\r\n\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.name=\"\"\r\n      this.load()\r\n    },\r\n    handleSelectionChange(val){\r\n      this.multipleSelection=val\r\n\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize;\r\n      this.load();\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum;\r\n      this.load();\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n\r\n  },\r\n  mounted(){\r\n\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AA4DA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,IAAA;MACAH,IAAA;MACAI,KAAA;MACAC,OAAA;MACAC,QAAA;MAEAC,iBAAA;MAAA;MACAC,iBAAA;MAAA;MACAC,QAAA;MACAC,YAAA;IAEA;EACA;EACAC,OAAA;IAGAC,KAAA;MAEA,KAAAb,OAAA,CAAAc,GAAA;QACAC,MAAA;UACAT,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QAKA;MACA,GAAAS,IAAA,CACAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAd,SAAA,GAAAc,GAAA,CAAAG,OAAA;QACA,KAAAf,KAAA,GAAAY,GAAA,CAAAZ,KAAA;QACAa,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,EACA;IACA;IAIAI,SAAA;MACA,IAAAC,GAAA,QAAAd,iBAAA,CAAAe,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,EAAA;MACAP,OAAA,CAAAC,GAAA,CAAAG,GAAA;MACA,KAAAtB,OAAA,CAAA0B,IAAA,yCAAAJ,GAAA,EAAAN,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAU,QAAA,CAAAC,OAAA;UACA,KAAAf,IAAA;QACA;UACA,KAAAc,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IAEAC,UAAAL,EAAA;MACA,KAAAzB,OAAA,CAAA+B,MAAA,oCAAAN,EAAA,EAAAT,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAU,QAAA,CAAAC,OAAA;UAEA,KAAAf,IAAA;QACA;UACA,KAAAc,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAG,MAAA;MACA,KAAA/B,IAAA;MACA,KAAAY,IAAA;IACA;IACAoB,sBAAAC,GAAA;MACA,KAAA1B,iBAAA,GAAA0B,GAAA;IAEA;IACAC,iBAA